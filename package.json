{
  "name": "procure",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all -l build:temporal --parallel dev:temporal dev:next",
    "dev:next": "next dev",
    "dev:temporal": "tsc-watch --noClear -p ./temporal/tsconfig.json --onSuccess \"node ./temporal/lib/worker\"",
    "build": "npm-run-all -l --parallel build:next build:temporal",
    "build:next": "next build",
    "build:temporal": "run-s rimraf rebuild",
    "start": "npm run dev",
    "start:worker": "node ./temporal/lib/worker",
    "lint": "eslint .",
    "rebuild": "tsc --build --force ./temporal/tsconfig.json",
    "rimraf": "rimraf ./temporal/lib"
  },
  "dependencies": {
    "@prisma/client": "^5.5.0",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@radix-ui/themes": "^2.0.0",
    "@tanstack/react-query": "^4.32.0",
    "@temporalio/activity": "^1.8.1",
    "@temporalio/client": "^1.8.1",
    "@temporalio/common": "^1.8.1",
    "@temporalio/worker": "^1.8.1",
    "@temporalio/workflow": "^1.8.1",
    "@types/node": "20.11.17",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "autoprefixer": "10.4.17",
    "axios": "^1.4.0",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "eslint": "8.56.0",
    "eslint-config-next": "13.5.6",
    "lucide-react": "^0.324.0",
    "next": "^13.4.12",
    "postcss": "8.4.35",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "5.3.3",
    "uuidv4": "^6.2.13",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@tailwindcss/forms": "^0.5.4",
    "@tsconfig/node16": "^16.1.0",
    "nodemon": "^3.0.1",
    "npm-run-all2": "^5.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "prisma": "^5.5.0",
    "rimraf": "^5.0.1",
    "tsc-watch": "^6.0.4"
  }
}
